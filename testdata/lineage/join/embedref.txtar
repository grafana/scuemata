# joinSchema is a reference to an embedded struct
-- in.cue --
import "github.com/grafana/thema"

thema.#Lineage
name: "embedref"
joinSchema: {
    EmbedRef
}
schemas: [{
	version: [0, 0]
	schema: {
		foo: string
	}
}]
lenses: []

EmbedRef: {
	refField1: string
	refField2: 42
}
