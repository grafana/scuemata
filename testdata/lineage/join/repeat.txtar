# Case where the joinSchema is exactly the same as a schema in the lineage
-- in.cue --
import "github.com/grafana/thema"

thema.#Lineage
name: "repeat"
joinSchema: {
    foo: string
}
schemas: [{
	version: [0, 0]
	schema: {
		foo: string
		someField: {
			EmbedRef
			run:  int32
			tell: bytes
			dat: [...string]
		}
	}
}]
lenses: []

EmbedRef: {
	refField1: string
	refField2: 42
}
-- out/bind --
Schema count: 1
Schema versions: 0.0
Lenses count: 0
