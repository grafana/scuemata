-- in.cue --
import (
	"time"
	"strings"

	"github.com/grafana/thema"
)

lin1: thema.#Lineage
lin1: name: "stringconstraint"
lin1: schemas: [{
	version: [0, 0]
	schema: {
		aString: string & strings.MinRunes(1)
		
		anObject: {
			aField: int
		}
	}
}]

lin2: thema.#Lineage
lin2: name: "stringconstraint"
lin2: schemas: [{
	version: [0, 0]
	schema: {
		aString: string & strings.MinRunes(2)
		
		anObject: {
			aField: int
		}
	}
}]
